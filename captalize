# Complete the solve function below.
def solve(s):
    t = s.split(' ')
    for i in range(len(t)):
        t[i] = t[i].capitalize()
    return ' '.join(t)

if __name__ == '__main__':
    fptr = open(os.environ['OUTPUT_PATH'], 'w')

    s = input()

    result = solve(s)
